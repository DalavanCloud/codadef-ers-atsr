<?xml version="1.0"?>
<cd:Record xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07" format="binary" name="ATS_TOA_1P_ADSR_pix_aatsr_rec_data" last-modified="2010-08-26">
  <cd:Field name="dsr_time">
    <cd:Time timeformat="binary_envisat_datetime">
      <cd:Description>Nadir UTC time in MJD format</cd:Description>
    </cd:Time>
  </cd:Field>
  <cd:Field name="attach_flag">
    <cd:Integer>
      <cd:Description>Attachment flag(set to 1 if all MDSRs corresponding to this ADSR are blank, set to 0 otherwise)</cd:Description>
      <cd:BitSize>8</cd:BitSize>
      <cd:NativeType>int8</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="spare_1">
    <cd:Raw>
      <cd:Description>Spare</cd:Description>
      <cd:BitSize>24</cd:BitSize>
    </cd:Raw>
    <cd:Hidden/>
  </cd:Field>
  <cd:Field name="img_scan_y">
    <cd:Integer>
      <cd:Description>image scan y coordinate</cd:Description>
      <cd:Unit>m</cd:Unit>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="instr_scan_num">
    <cd:Array>
      <cd:Description>instrument scan number</cd:Description>
      <cd:Dimension>512</cd:Dimension>
      <cd:Integer>
        <cd:BitSize>16</cd:BitSize>
        <cd:NativeType>uint16</cd:NativeType>
      </cd:Integer>
    </cd:Array>
  </cd:Field>
  <cd:Field name="pix_num">
    <cd:Array>
      <cd:Description>pixel number</cd:Description>
      <cd:Dimension>512</cd:Dimension>
      <cd:Integer>
        <cd:BitSize>16</cd:BitSize>
        <cd:NativeType>uint16</cd:NativeType>
      </cd:Integer>
    </cd:Array>
  </cd:Field>
</cd:Record>
