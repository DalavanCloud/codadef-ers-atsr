<?xml version="1.0"?>
<cd:Record format="binary" name="ATS_AR__2P_MDSR_lst_small_aatsr_rec_data" last-modified="2011-01-25" xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07">
  <cd:Field name="dsr_time">
    <cd:Time timeformat="binary_envisat_datetime">
      <cd:Description>Nadir UTC time in MJD format</cd:Description>
    </cd:Time>
  </cd:Field>
  <cd:Field name="quality_flag">
    <cd:Integer>
      <cd:Description>Quality Indicator (-1 for blank MDSR, 0 otherwise)</cd:Description>
      <cd:BitSize>8</cd:BitSize>
      <cd:NativeType>int8</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="spare_1">
    <cd:Raw>
      <cd:Description>Spare</cd:Description>
      <cd:BitSize>24</cd:BitSize>
    </cd:Raw>
    <cd:Hidden/>
  </cd:Field>
  <cd:Field name="lat">
    <cd:Integer>
      <cd:Description>Latitude of cell</cd:Description>
      <cd:Unit>1e-6 degrees_north</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="1000000.000000">
        <cd:Unit>degrees_north</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="lon">
    <cd:Integer>
      <cd:Description>Longitude of cell</cd:Description>
      <cd:Unit>1e-6 degrees_east</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="1000000.000000">
        <cd:Unit>degrees_east</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>int32</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="m_actrk_pix_num">
    <cd:Integer>
      <cd:Description>Mean across-track pixel number</cd:Description>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>int16</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="m_lst">
    <cd:Integer>
      <cd:Description>mean land surface temperature</cd:Description>
      <cd:Unit>K/100</cd:Unit>
      <cd:Conversion numerator="1.000000" denominator="100.000000">
        <cd:Unit>K</cd:Unit>
      </cd:Conversion>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>int16</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="pix_lst">
    <cd:Integer>
      <cd:Description>Number of pixels in land surface temperature average</cd:Description>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>int16</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="m_ndvi">
    <cd:Integer>
      <cd:Description>mean NDVI</cd:Description>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>int16</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="pix_ndvi">
    <cd:Integer>
      <cd:Description>Number of pixels in NDVI average</cd:Description>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>uint16</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="ast_conf_flags">
    <cd:Array>
      <cd:Description>AST confidence word</cd:Description>
      <cd:Dimension>2</cd:Dimension>
      <cd:Integer>
        <cd:BitSize>16</cd:BitSize>
        <cd:NativeType>uint16</cd:NativeType>
      </cd:Integer>
    </cd:Array>
  </cd:Field>
</cd:Record>
